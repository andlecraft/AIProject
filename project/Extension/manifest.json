{
    "manifest_version": 3,
    "name": "What is your credibility?",
    "version": "1.0",
    "description": "A simple Chrome extension example.",
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": "Sample.png"
    },
    "permissions": [
      "tabs"
    ]
  }
  